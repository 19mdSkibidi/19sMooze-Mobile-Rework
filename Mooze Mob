-- WS/JP Logic
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid") or nil

getgenv().WalkSpeedValue = tonumber(wsBox.Text) or 587
getgenv().JumpPowerValue = tonumber(jpBox.Text) or 50
local speedEnabled = true
local jumpEnabled = true

local function applySpeed()
    if not humanoid then return end
    
    humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
        if speedEnabled then
            humanoid.WalkSpeed = getgenv().WalkSpeedValue
        end
    end)
    
    humanoid.WalkSpeed = speedEnabled and getgenv().WalkSpeedValue or 16
end

local function applyJump()
    if not humanoid then return end
    
    humanoid:GetPropertyChangedSignal("JumpPower"):Connect(function()
        if jumpEnabled then
            humanoid.JumpPower = getgenv().JumpPowerValue
        end
    end)
    
    humanoid.JumpPower = jumpEnabled and getgenv().JumpPowerValue or 50
end

player.CharacterAdded:Connect(function(char)
    humanoid = nil
    char:WaitForChild("Humanoid")
    humanoid = char:FindFirstChildOfClass("Humanoid")
    applySpeed()
    applyJump()
end)

toggleGuiButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = not mainFrame.Visible
end)

toggleWSButton.MouseButton1Click:Connect(function()
    speedEnabled = not speedEnabled
    toggleWSButton.Text = speedEnabled and "WS: ON" or "WS: OFF"
    applySpeed()
end)

toggleJPButton.MouseButton1Click:Connect(function()
    jumpEnabled = not jumpEnabled
    toggleJPButton.Text = jumpEnabled and "JP: ON" or "JP: OFF"
    applyJump()
end)

wsBox.FocusLost:Connect(function()
    getgenv().WalkSpeedValue = tonumber(wsBox.Text) or 250
    applySpeed()
end)

jpBox.FocusLost:Connect(function()
    getgenv().JumpPowerValue = tonumber(jpBox.Text) or 250
    applyJump()
end)

-- Initial setup
applySpeed()
applyJump()
